<div>
    <% if(rs.length > 0) { %>
    <table border="1">
        <tr><th>Name</th></tr>
        <% for (var i=0; rs.length > i; i++) { %>
        <tr>
            <td><a href="/Weapon/?id=<%= rs[i].id %>"> <%= rs[i].name %></a></td>
        </tr>
        <% } %>
    </table>
    <% } else { %>
    No Weapon are entered into the Weapon database table.
    <% } %>
</div>

<div style="margin-top:10px;">
    <a href="/Weapon/create">Create New Weapon</a>
</div>